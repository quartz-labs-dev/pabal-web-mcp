{
  "name": "pabal-web-mcp",
  "version": "1.2.2",
  "type": "module",
  "description": "MCP server for ASO data management with shared types and utilities",
  "author": "skyu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quartz-labs-dev/pabal-web-mcp"
  },
  "homepage": "https://github.com/quartz-labs-dev/pabal-web-mcp#readme",
  "bugs": {
    "url": "https://github.com/quartz-labs-dev/pabal-web-mcp/issues"
  },
  "keywords": [
    "mcp",
    "aso",
    "app-store",
    "google-play",
    "localization"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "pabal-web-mcp": "./dist/bin/mcp-server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/bin/mcp-server.ts --format esm --dts",
    "dev": "tsx src/bin/mcp-server.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@googleapis/androidpublisher": "^33.2.0",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "appstore-connect-sdk": "^1.3.2",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tsup": "^8.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.5"
  }
}
